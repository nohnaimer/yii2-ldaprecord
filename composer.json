{
    "name": "nohnaimer/yii2-ldaprecord",
    "description": "Yii2 extension for library LdapRecord",
    "keywords": [
        "yii2",
        "extension",
        "ldap",
        "active directory",
        "openldap",
        "ldaprecord"
    ],
    "homepage": "https://github.com/nohnaimer/yii2-ldaprecord",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Maksim Astapenko",
            "email": "actapenko@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "2.0.*",
        "directorytree/ldaprecord": "2.1.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "nohnaimer\\ldaprecord\\": "src"
        }
    },
    "repositories": {
        "asset": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
